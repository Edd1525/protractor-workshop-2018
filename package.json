{
  "name": "workshop-protractor",
  "version": "1.0.0",
  "description": "This is a Workshop about Protractor",
  "main": "index.js",
  "scripts": {
    "test:saucelabs": "npm run build && protractor dist/protractor/saucelabs.config.js",
    "test": "npm run test:saucelabs",
    "clean": "rm -rf dist && rm -rf reports",
    "build": "npm run lint && npm run clean && tsc",
    "test:local": "npm run build && protractor dist/protractor/local.config.js",
    "test:headless": "npm run build && protractor dist/protractor/headless.config.js",
    "postinstall": "webdriver-manager update --gecko false",
    "lint": "tslint --project tsconfig.json protractor/**/*.ts test/**/*.ts src/**/*.ts"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "protractor"
  ],
  "author": "[edward] <[edward2515@gmail.com]> ([edd1525])",
  "license": "MIT",
  "dependencies": {
    "protractor": "^5.4.1"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.5",
    "jasmine-awesome-report": "0.0.3",
    "jasmine-spec-reporter": "^4.2.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.3"
  }
}